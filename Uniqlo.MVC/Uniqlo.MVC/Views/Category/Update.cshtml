@model Category;

<form method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input class="form-control" asp-for="Name" />
    </div>
    <div class="form-group">
        <label asp-for="Id"></label>
        <input class="form-control" asp-for="Id" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>